<p>
  <%= link_to t('.new_question'), new_admin_test_question_path(@test) %>
</p>

<% if @test.questions.present? %>
  <table>
    <tr>
      <th><%= t('.table.body') %></th>
      <th colspan="3"><%= t('.table.actions') %></th>
    </tr>

    <%= render partial: 'admin/questions/question', collection: @test.questions %>

  </table>
<% else %>
  <p><%= t('.no_questions_available') %></p>
<% end %>
